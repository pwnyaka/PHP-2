<h2>Каталог</h2>

<div class="wrapper">
    {% for item in catalog %}
    <h2><a href="/catalog/card/?id={{ item.id }}">{{ item.prodName }}</a></h2>
    <img src="/img/{{ item.imgName }}" alt="" style="width: 500px;">
    <p>{{ item.description }}</p>
    <p>Цена: {{ item.cost }}</p>
    <p>Просмотры: {{ item.views }}</p>
    <hr>
    {% endfor %}
</div>

<script src="/js/catalog.js"></script>